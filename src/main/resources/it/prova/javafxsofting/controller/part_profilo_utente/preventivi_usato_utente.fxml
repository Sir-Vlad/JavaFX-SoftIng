<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="800.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="it.prova.javafxsofting.controller.part_profilo_utente.PreventivoUsatoUtenteController">
    <children>
        <VBox layoutX="10.0" layoutY="10.0" prefHeight="800.0" prefWidth="900.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Preventivi Usato">
                            <HBox.margin>
                                <Insets left="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Text>
                    </children>
                </HBox>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <children>
                        <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0"
                                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="idColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-Infinity" prefWidth="50.0" resizable="false"
                                             text="ID"/>
                                <TableColumn fx:id="modelloColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-1.0" prefWidth="140.0" text="Modello"/>
                                <TableColumn fx:id="preventivoColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="120.0" text="Preventivo"/>
                                <TableColumn fx:id="prezzoColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-1.0" prefWidth="130.0" text="Prezzo"/>
                                <TableColumn fx:id="statoColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-1.0" prefWidth="140.0" text="Stato"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="900.0"
            xmlns="http://javafx.com/javafx/21">
    <children>
        <VBox layoutX="10.0" layoutY="10.0" prefHeight="800.0" prefWidth="900.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ordini">
                            <HBox.margin>
                                <Insets left="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Text>
                    </children>
                </HBox>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <children>
                        <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0"
                                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="idColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-Infinity" prefWidth="50.0" resizable="false"
                                             text="ID"/>
                                <TableColumn fx:id="modelloColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-1.0" prefWidth="130.0" resizable="false"
                                             text="Modello"/>
                                <TableColumn fx:id="prezzoBaseColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="120.0" resizable="false"
                                             text="Prezzo Base"/>
                                <TableColumn fx:id="prezzoOptionalsColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="130.0" resizable="false"
                                             text="Prezzo Aggiuntivo"/>
                                <TableColumn fx:id="scontoColumn" maxWidth="1.7976931348623157E308"
                                             minWidth="-1.0" prefWidth="120.0" resizable="false"
                                             text="Sconto"/>
                                <TableColumn fx:id="totPrezzoColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="120.0" resizable="false" text="Totale"/>
                                <TableColumn fx:id="dataEmissioneColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="130.0" resizable="false"
                                             text="Data Emissione"/>
                                <TableColumn fx:id="confermaColumn"
                                             maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                             prefWidth="100.0" resizable="false" text="Conferma"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
